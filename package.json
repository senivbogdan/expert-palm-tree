{
  "name": "frontend-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.24.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hook-form": "^7.20.5",
    "react-is": "^16.13.1",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^5.0.1",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "styled-components": "^5.3.5",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "jest-canvas-mock": "^2.3.1",
    "jest-code-coverage-protection": "^1.0.3",
    "mockdate": "^3.0.5",
    "node-sass": "^7.0.1",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^10.0.2"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "jest": {
    "clearMocks": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/styleMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@here)",
      "cypress/(?!@here)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
